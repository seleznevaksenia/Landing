nav.-nav
    -var items = [{name:"Home",href:"#"},{name:"About Us",href:"#"},{name:"Our Approch",href:"#"},{name:"Our Work",href:"#"}]
    each item in items
        if item.name === "Home"
            a.nav__link.nav__link--active(href=item.href)=item.name
        else
            a.nav__link(href=item.href)=item.name